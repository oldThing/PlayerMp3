    <attr name="text" format="string"></attr>
    <attr name="text_size" format="dimension"></attr>
    <attr name="ic_color" format="color"></attr>

    <declare-styleable name="cursorTab">
        <attr name="ic_icon"></attr>
        <attr name="text"></attr>
        <attr name="text_size"></attr>
